tit = 's = -1.861 @_{-0.058}^{+0.065} '
 f(x) = -0.861 * x + 6.263 
 a=3.4906606533561368
 set term postscript enhanced color
 set out 'plot.eps'
 set multiplot layout 1,2 title tit
 set size square
  set xr [-4:0]
  set yr [1:a*1.1]
  p "./plot_data" u 1:2 w p pt 7 ps 0.3 t "Data", f(x) lc 2
  set yr [-0.1:0.1 ]
  set lmargin 6.3
 p "./plot_data" u 1:($2-f($1)) w l t "f(x) residuals" lc 2
